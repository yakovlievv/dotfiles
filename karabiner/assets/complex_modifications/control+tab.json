{
  "title": "Control+Tab as Command+Tab",
  "rules": [
    {
      "description": "Control+Tab works as Command+Tab",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "tab",
            "modifiers": {
              "mandatory": ["control"],
              "optional": ["any"]
            }
          },
          "to": [
            {
              "key_code": "tab",
              "modifiers": ["command"]
            }
          ]
        }
      ]
    }
  ]
}
