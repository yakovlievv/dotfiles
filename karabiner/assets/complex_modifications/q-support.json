{
  "title": "q",
  "rules": [
    {
      "description": "Option+Q behaves as Command+Q",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "q",
            "modifiers": {
              "mandatory": ["left_option"],
              "optional": ["any"]
            }
          },
          "to": [
            {
              "key_code": "q",
              "modifiers": ["left_command"]
            }
          ]
        }
      ]
    }
  ]
}
